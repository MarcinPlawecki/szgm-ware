<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="szgm.jednostka.model.Jednostka" 
        table="jednostka" catalog="gm">
 
        <id name="id" type="long">
            <column name="ID" />
            <generator class="sequence" >
                <param name="sequence">jednostka_seq</param>
            </generator>
        </id>
        <property name="nazwa" type="string">
            <column name="NAZWA" length="10" not-null="true" />
        </property>
    </class>
</hibernate-mapping>